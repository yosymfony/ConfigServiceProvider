{
    "name": "yosymfony/config-serviceprovider",
    "description": "A configuration ServiceProvider for Silex based on Symfony Config component with support for yaml and toml",
    "keywords": ["silex", "config", "configuration", "yaml", "toml"],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Puertas",
            "email": "vpgugr@gmail.com",
            "homepage": "http://yosymfony.com"
        }
    ],
    "require": {
        "silex/silex": "~1.0",
        "symfony/config": "2.4.*@dev",
        "yosymfony/toml": "0.1.*",
        "symfony/yaml": "2.4.*@dev",
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-0": { "Yosymfony\\Silex\\ConfigServiceProvider": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}